#include<iostream>
#include<string>
using namespace std;
void populateDatabase(char a[][3]);
void printDatabase(char a[][3]);
int main()
{
  char a[15][3];
  cout<<"input the name,age and score\n";
  populateDatabase(a);
  printDatabase(a);
  return 0;
}

void populateDatabase(char a[][3])
{
 
   for(int i=0;i<3;i++)
    for(int j=0;j<3;j++)
	cin>>a[i][j];
}

void printDatabase(char a[][3])
{
  for(int i=0;i<3;i++)
  {
    for(int j=0;j<3;j++)
    {
      cout<<a[i][j]<<"    ";
    }
    cout<<endl;
  }
  
}
